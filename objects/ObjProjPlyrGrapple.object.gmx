else
{
    move_towards_point(ObjEnvPlanet.x, ObjEnvPlanet.y, speed);
    
    //grapple returned
    if (y &gt;= ObjEnvPlanet.y)
    {
        //returned
        if (instance_exists(grappleTarget))
        {
            with (grappleTarget)
            {
                ScrJunkCapReturned();
                instance_destroy();
            }

            instance_destroy();
        }
        //not returned
        else
        {
            instance_destroy();
        }
    }
}

//image angle
image_angle = direction - 90;
